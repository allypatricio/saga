<div class="container">
  <div class="dashboard">
    <div class="header-tours">
      <p>My Tours</p>
    </div>
    <div class="new-message">
      <div class="new-message-button">
      <%= link_to "Add new tour", new_tour_path, class: "btn btn-form-blue new-tour-margin" %>
      </div>
    </div>
    <% @tours.each do |tour| %>
      <%= render "tour", tour: tour %>
    <%end%>
  </div>
</div>
